{
  "compilerOptions": {
    "target": "es2017",
    "module": "es2015",
    "moduleResolution": "node",
    "lib": ["es2017", "dom"],
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "plugins": [
      {
        "name": "typescript-lit-html-plugin",
        "tags": ["html", "css", "template"]
      }
    ]
  }
}
